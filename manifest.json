{
  "name": "Comingle Modify",
  "short_name": "Modify",
  "author": "Comingle",
  "description": "A great app for the Comingle toy!",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "logoicon-16.png",
           "32": "logoicon-32.png",
           "64": "logoicon-64.png",
          "128": "logoicon-128.png",
          "256": "logoicon-256.png"
  },
  "permissions": [
    "serial",
    "storage",
    "usb",
    "http://*/",
    "https://*/"
  ]
}
